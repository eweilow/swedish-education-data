{
  "title": "Bygg- och anläggningsprogrammet",
  "code": "BA",
  "url": "https://www.skolverket.se/laroplaner-amnen-och-kurser/gymnasieutbildning/gymnasieskola/programstruktur-och-examensmal/bygg-och-anlaggningsprogrammet",
  "typeOfSchooling": "UPPER_SECONDARY_EDUCATION",
